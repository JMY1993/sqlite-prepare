{
  "name": "sqlite-prepare",
  "version": "0.0.9",
  "description": "A type-safe SQL query builder for Cloudflare D1",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "vitest",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "cloudflare",
    "d1",
    "sql",
    "query-builder",
    "sqlite",
    "parameterized"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.24",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^11.9.1",
    "rollup": "^4.40.1",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "vitest": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JMY1993/sqlite-prepare"
  },
  "bugs": {
    "url": "https://github.com/JMY1993/sqlite-prepare"
  },
  "homepage": "https://github.com/JMY1993/sqlite-prepare#readme"
}